{
  "name": "gromo",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:load": "artillery run tests/load/config.yml",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "security:audit": "npm audit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node src/database/migrationRunner.ts",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:status": "npm run migrate status",
    "db:seed": "ts-node scripts/seed.ts",
    "docker:build": "docker build -t gromo:latest .",
    "docker:run": "docker run -p 3000:3000 gromo:latest",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "docs:generate": "swagger-jsdoc -d src/config/swagger.ts -o docs/api.json src/api/*.ts",
    "docs:serve": "swagger-ui-serve docs/api.json",
    "prepare": "husky",
    "cypress:open": "CYPRESS=true cypress open",
    "cypress:run": "CYPRESS=true cypress run",
    "seed": "ts-node scripts/seed.ts",
    "start:test": "NODE_ENV=development CYPRESS=true ts-node src/index.ts"
  },
  "keywords": [
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "description": "A TypeScript-based project built from documentation specifications",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.24",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "artillery": "^2.0.23",
    "cypress": "^14.4.0",
    "cypress-axe": "^1.5.0",
    "cypress-real-events": "^1.11.0",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^30.0.0-beta.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google-cloud/dialogflow": "^7.0.1",
    "@google-cloud/speech": "^7.1.0",
    "@google-cloud/vision": "^5.1.0",
    "@types/express": "^4.17.22",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.53.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "googleapis": "^149.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "node-cron": "^4.0.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
